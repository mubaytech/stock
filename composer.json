{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "beyondcode/laravel-websockets": "^1.11",
        "fideloper/proxy": "^4.4",
        "franzose/closure-table": "^6.1",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.5",
        "joselfonseca/lighthouse-graphql-passport-auth": "^6.0",
        "kutia-software-company/larafirebase": "^1.0",
        "laravel/framework": "^8.12",
        "laravel/passport": "^10.1",
        "laravel/socialite": "^5.1",
        "laravel/telescope": "^4.4",
        "laravel/tinker": "^2.5",
        "mll-lab/graphql-php-scalars": "4",
        "mll-lab/laravel-graphql-playground": "^2.5",
        "nuwave/lighthouse": "^5.3",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "~4.0",
        "tenancy/tenancy": "^1.3"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.9",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Database\\Teneant\\Factories\\": "database/tenant/factories/",
            "Database\\Teneant\\Seeders\\": "database/tenant/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
